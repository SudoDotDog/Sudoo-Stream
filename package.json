{
	"name": "@sudoo/stream",
	"version": "1.0.0",
	"description": "Pipe Stream for JS",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SudoDotDog/Sudoo-Stream.git"
	},
	"author": "WMXPY",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/SudoDotDog/Sudoo-Stream/issues"
	},
	"homepage": "https://github.com/SudoDotDog/Sudoo-Stream#readme",
	"devDependencies": {
		"@sudoo/io": "^1.8.0",
		"@sudoo/triforce": "^3.11.0"
	},
	"nyc": {
		"extension": [
			".ts"
		],
		"include": [
			"script/**/*.ts",
			"src/**/*.ts"
		],
		"instrument": true,
		"reporter": [
			"json",
			"text-summary"
		],
		"report-dir": "coverage",
		"sourceMap": true
	}
}